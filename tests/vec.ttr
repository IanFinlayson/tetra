
def getPrimes( ) [int]:
  # some of them anyway
  return [2, 3, 5, 7, 11, 13, 17, 19, 23, 27, 31]

def sum(nums [int]) int:
  i = 0
  sum = 0
  while i < len(nums):
    sum += nums[i]
    i += 1
  return sum

def main( ):
  ps = getPrimes( )
  print("Sum of first ", len(ps), " primes is ", sum(ps))



