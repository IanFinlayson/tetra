def main():
   x = [2,4,6]
   print("Just created x and x = ", x, "\n")
   y = call(x)
   print("Just created y, set y = call(x),  and y = ", y, "\n")
   x[1] = 10
   print("Just set x[1] = 10 and x = ", x, "\n")
   print("and y = ", y, "\n")

def call(a [int]) [int]:
   print("In call() and a = ", a, "\n")
   return a
