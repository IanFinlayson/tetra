def main():
   passer = [1,2,3]
   print("Just created passer and passer = ", passer, "\n")
   cally(passer)
   print("In main, just returned from cally(passer) and passer = ",passer, "\n")
   print("passer[0] + passer[1] + passer[2] = ", passer[0] + passer[1] + passer[2], "\n")

def cally(argx [int]):
   print("Just entered cally() and argx = ", argx, "\n")
   caller(argx)
   print("In cally(), just returned from caller(argx), and argx = ", argx, "\n")

def caller(argy [int]):
   print("Just entered caller() and argy = ", argy, "\n")
   argy[0] = 10
   print("Just set argy[0] = 10 and argy = ", argy, "\n")
   argy[1] = 20
   print("Just set argy[1] = 20 and argy = ", argy, "\n")
   argy[2] = 30
   print("Just set argy[2] = 30 and argy = ", argy, "\n")
